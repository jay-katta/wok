msgstr "Entrar"

msgid "Logging in..."
msgstr "Entrando..."

msgid "Peers"
msgstr "Peers"

msgid "Searching"
msgstr "Procurando"

msgid "No peers found."
msgstr "Nenhum peer encontrado."

msgid "Log out"
msgstr "Sair"

msgid "Help"
msgstr "Ajuda"

msgid "About"
msgstr "Sobre"

